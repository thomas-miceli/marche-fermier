<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<fx:root fx:id="contentVBox" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox prefHeight="0.0" prefWidth="600.0">
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Nouveau Trader" />
            </children>
        </HBox>
        <HBox prefHeight="45.0" prefWidth="600.0">
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Nom :   " />
                <TextField fx:id="Nom" prefHeight="31.0" prefWidth="119.0" />
            </children>
        </HBox>
        <Text fx:id="textType" strokeType="OUTSIDE" strokeWidth="0.0" text="   Participants : " />
      <VBox prefHeight="200.0" prefWidth="100.0" fx:id="Participants" />

        <HBox prefHeight="12.0" prefWidth="600.0">
            <children>
                <Button mnemonicParsing="false" onAction="#newTrader" prefHeight="31.0" prefWidth="100.0" text="Valider" />
                <Canvas height="22.0" width="200.0" />
                <Text fx:id="alertText" strokeType="OUTSIDE" strokeWidth="0.0" />
            </children>
        </HBox>
    </children>
</fx:root>
